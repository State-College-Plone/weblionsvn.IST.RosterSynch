    directory="RosterSynch/skins/rostersynch_templates" />
  <object name="rostersynch_scripts"
    meta_type="Filesystem Directory View"
    directory="Products.RosterSynch:skins/rostersynch_scripts"/>
  <object name="rostersynch_styles"
    meta_type="Filesystem Directory View"
    directory="Products.RosterSynch:skins/rostersynch_styles"/>
